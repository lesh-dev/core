<?php
    global $SETTINGS;
    $usr_prefix = "{$SETTINGS["datadir"]}auth/usr/";
    $list = glob("$usr_prefix*.usr");
    foreach ($list as $key=>$value)
    {
        $value = str_replace($usr_prefix, "", $value);
        $value = str_replace(".usr", "", $value);
        $taglist[$value] = $value;
    }
    $tagname = "Владелец раздела";
?>