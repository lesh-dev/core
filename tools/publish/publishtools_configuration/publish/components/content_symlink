#!/usr/bin/env bash
# Component generated from template on Mon Sep 17 20:32:26 MSK 2012
# Please don't forget to commit all changes to repo

if [ -z "$clone_root" ] ; then
    echo "Clones root is not set"
    exit 1
fi

if [ -z "$site" ] ; then
    echo "Site is not set"
    exit 1
fi

type=symlink
location=$clone_root/content-$site/content
local_target=./content
